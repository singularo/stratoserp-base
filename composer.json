{
  "name": "singularo/stratoserp_base",
  "description": "StratosERP installation profile",
  "type": "drupal-profile",
  "homepage": "https://github.com/singularo/stratoserp_base",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/calendar": "1.*@dev",
    "drupal/core": "8.7.*",
    "drupal/duration_field": "2.*@dev",
    "drupal/entity_reference_revisions": "1.*@dev",
    "drupal/eva": "^2.0",
    "drupal/field_group": "3.x-dev",
    "drupal/geolocation": "1.*@dev",
    "drupal/paragraphs": "1.x-dev",
    "drupal/reference_table_formatter": "1.*@dev",
    "drupal/serial": "1.*@dev",
    "drupal/views_data_export": "1.*@dev",
    "drupal/views_templates": "1.*@dev",
    "drupal/xero": "1.*@dev",
    "drush/drush": "8.*"
  },
  "minimum-stability": "dev",
  "config": {
    "bin-dir": "bin/",
    "discard-changes": true
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  }
}
